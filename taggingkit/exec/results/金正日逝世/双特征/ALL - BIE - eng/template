# Unigram
U001:%x[-2,0]
U002:%x[-1,0]
U003:%x[0,0]
U004:%x[1,0]
U005:%x[2,0]
U007:%x[-1,0]/%x[0,0]
U008:%x[0,0]/%x[1,0]

U101:%x[-2,1]
U102:%x[-1,1]
U103:%x[0,1]
U104:%x[1,1]
U105:%x[2,1]
U107:%x[-2,1]/%x[-1,1]
U108:%x[-1,1]/%x[0,1]
U109:%x[0,1]/%x[1,1]
U110:%x[1,1]/%x[2,1]

U200:%x[-2,3]
U201:%x[-1,3]
U202:%x[0,3]
U203:%x[1,3]
U204:%x[2,3]

