# Unigram
#第0列
U001:%x[-2,0]
U002:%x[-1,0]
U003:%x[0,0]
U004:%x[1,0]
U005:%x[2,0]
U007:%x[-1,0]/%x[0,0]
U008:%x[0,0]/%x[1,0]

#第2列
U101:%x[-2,2]
U102:%x[-1,2]
U103:%x[0,2]
U104:%x[1,2]
U105:%x[2,2]

#第3列
U301:%x[-1,3]
U302:%x[0,3]
U303:%x[1,3]
U304:%x[-1,3]/%x[0,3]
U305:%x[0,3]/%x[1,3]
U306:%x[-1,3]/%x[0,3]/%x[1,3]

#第4列
U401:%x[-2,4]
U402:%x[-1,4]
U403:%x[0,4]
U404:%x[1,4]
U405:%x[2,4]

